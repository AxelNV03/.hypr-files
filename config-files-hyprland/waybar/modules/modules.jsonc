    "custom/disk-total": {
        "exec": "lsblk -o NAME,SIZE | grep nvme0n1p5 | awk '{print $2}'",
        "interval": 60,
        "format": " {} üñ¥",
        "tooltip": "Espacio total del disco"
    },
    "custom/disk-used": {
        "exec": "df -h | grep '/dev/nvme0n1p5' | awk '{print $3}'",
        "interval": 30,
        "format": " {} üñ¥",
        "tooltip": "Espacio usado en el disco"
    },
    "custom/disk-available": {
